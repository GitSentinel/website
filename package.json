{
  "name": "gdsc-website",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .jsx --fix",
    "pretty": "pnpm prettier --write .",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.0",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": ">=5.16.0",
    "eslint-plugin-react": "^7.31.10",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "simple-git-hooks": "^2.8.1",
    "vite": "^3.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm precommit"
  },
  "lint-staged": {
    "*.(jsx|js)": [
      "pnpm lint",
      "pnpm pretty"
    ],
    "*.scss": [
